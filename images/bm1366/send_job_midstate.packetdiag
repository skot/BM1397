packetdiag {
  0-7: 0x55;
  8-15: 0xAA;
  16: 0;
  17-18: 1;
  19: 0;
  20-21: 0;
  22-23: 1;
  24-31: 22+Midstates Count*32;
  32-39: Job ID;
  40-47: Midstates Count;
  48-63: Starting Nonce [colheight = 2];
  80-95: NBits [colheight = 2];
  112-127: NTime [colheight = 2];
  144-159: Merkle Root End (last 4 bytes only) [colheight = 2];
  176-191: Midstates (32 bytes per Midstate, swap 32-bit) [colheight = 4];
  240-255: CRC16;
}